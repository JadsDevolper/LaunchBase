{% extends "layout.njk" %}

{% block content %}
  <div class="card">
    <table>
      <thead>
        <tr>
          <th>Pets</th>
          <th>Habilidades</th>
        </tr>
      </thead>

      <tbody>

        {% for dog in dogs %}
          <tr>
            <td>{{ dog.name}}</td>
            <td>{{ dog.abiliity}}</td>
          </tr>
        {% endfor %}


      </tbody>
    </table>
  </div>

{% endblock content %}